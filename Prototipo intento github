//Proyecto Sistema de Gesti√≥n de Procesos del grupo 9
#include <iostream>
using namespace std;

struct Proceso {
    int id;
    string nombre;
    int prioridad;
};

struct Nodo {
    Proceso dato;
    Nodo* siguiente;
};

void insertarProceso(Nodo*& cabeza, Proceso p) {
    Nodo* nuevoNodo = new Nodo();
    nuevoNodo->dato = p;
    nuevoNodo->siguiente = cabeza;
    cabeza = nuevoNodo;
}
